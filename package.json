{
  "name": "stylestage-sass",
  "version": "1.1.1",
  "description": "A minimal starter for Style Stage submissions that includes the source Sass used to create the base Style Stage theme, and the source HTML.",
  "main": "index.html",
  "scripts": {
    "scss": "sass --no-source-map sass:docs",
    "watch": "sass --no-source-map --watch sass:docs",
    "serve": "browser-sync start --server docs --files docs --port 1337",
    "start": "npm-run-all scss --parallel watch serve"
  },
  "keywords": [
    "style stage",
    "sass"
  ],
  "author": "5t3ph",
  "license": "ISC",
  "dependencies": {
    "browser-sync": "^3.0.4",
    "npm-run-all": "^4.1.5",
    "sass": "^1.26.10",
    "purgecss": "^8.0.0",
    "stylelint": "^17.3.0",
    "stylelint-config-standard": "^40.0.0",
    "uncss": "^0.17.3"
  }
}
